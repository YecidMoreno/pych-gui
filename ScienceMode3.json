{
    "plugins": {
        "path": [
            "./plugins/",
            "/home/inception/git/pych/release/x86_64-linux-gnu/plugins/",
            "/home/inception/git/pych_plugins/release/x86_64-linux-gnu/plugins/"
        ],
        "commIO": {
            "SerialIO": "libSerialIO.so"
        },
        "deviceIO": {
            "ScienceMode3": "ScienceMode3.so",
            "Channel_sm3": "Channel_sm3.so"
        },
        "controlIO": {
            "Functional": "Functional.so"
        }
    },
    "commIO": {},
    "deviceIO": {
        "fes0": {
            "type": "ScienceMode3",
            "config": {
                "iface": "/dev/ttyUSB0"
            }
        },
        "fes0_r": {
            "type": "Channel_sm3",
            "config": {
                "device": "fes0",
                "channel": 0,
                "mA": 10,
                "pw": 200,
                "Hz": 60,
                "ramp": 0,
                "max_mA": 40,
                "max_pw": 250,
                "mode": "mA"
            }
        }
    },
    "controlIO": {
        "fnc0": {
            "type": "Functional",
            "config": {
                "in": [],
                "out": [
                    "fes0_r"
                ],
                "eval": [
                    "y0 := 4;",
                    "y0 := 2*(1+sin(2*M_PI*.5*t));",
                    "y1 := t;"
                ],
                "ts": "10ms",
                "wakeup": "0ms",
                "thread": "_thr_control0"
            }
        }
    },
    "log": {
        "enable": false,
        "path": "logs/"
    },
    "robot": {
        "name": "ExoTAO",
        "type": "biped",
        "sensors": [
            "enc_epos5"
        ],
        "actuators": [
            "motor_epos5"
        ],
        "controllers": [
            "SimplePD"
        ]
    },
    "core": {
        "max_run_time": "5s"
    }
}